{
  "name": "web_front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p ${PORT}",
    "build": "next build && next export && tailwindcss styles/globals.css -o out/globals.css",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-gh-pages": "rm -rf node_modules/.cache && next build && next export && tailwindcss styles/globals.css -o out/globals.css && touch out/.nojekyll && git add -f out/ && git checkout -b temp-for-deploy-gh-pages && git commit -m \"Deploy Next.js to gh-pages\" && git subtree split --prefix out -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages && git checkout main && git branch -D temp-for-deploy-gh-pages"
  },
  "dependencies": {
    "apexcharts": "^3.35.0",
    "axios": "^0.27.2",
    "chart.js": "^3.7.1",
    "i18next": "^21.6.16",
    "moment-timezone": "^0.5.34",
    "mqtt": "^4.3.7",
    "next": "12.1.5",
    "react": "18.0.0",
    "react-apexcharts": "^1.4.0",
    "react-chartjs-2": "^4.1.0",
    "react-contextmenu": "^2.14.0",
    "react-dom": "18.0.0",
    "react-hook-form": "^7.31.3",
    "react-i18next": "^11.16.7",
    "react-icons": "^4.3.1",
    "react-timezone-select": "^1.3.1",
    "react-tooltip": "^4.2.21",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@types/node": "17.0.26",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.2",
    "autoprefixer": "^10.4.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-storybook": "^0.5.11",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "react-docgen-typescript-loader": "^3.7.2",
    "sb": "^6.4.22",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.3"
  }
